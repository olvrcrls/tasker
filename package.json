{
  "name": "tasker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npx tailwind build ./src/assets/css/styles.css -c ./tailwind.js -o ./public/css/main.css --no-autoprefixer && vue-cli-service serve:bs",
    "build": "npx tailwind build ./src/assets/css/styles.css -c ./tailwind.js -o ./public/css/main.css --no-autoprefixer && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "serve:bs": "vue-cli-service serve:bs"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/vue-fontawesome": "^0.1.5",
    "moment": "^2.24.0",
    "random-words": "^1.1.0",
    "register-service-worker": "^1.5.2",
    "vue": "^2.6.6",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-pwa": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "tailwindcss": "^0.7.4",
    "vue-cli-plugin-browser-sync": "^1.0.0-rc3",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
